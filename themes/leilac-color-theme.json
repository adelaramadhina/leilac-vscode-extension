		"list.inactiveFocusBackground": "#7D7796",
		"list.dropBackground": "#6959aa80",
		"list.focusBackground": "#7D7796",

		"dropdown.background": "#464258",
		"dropdown.foreground": "#F8F8F2",
		"dropdown.border": "#6B6683",
		
		"editor.background": "#5A5475",
		"editorCursor.foreground": "#F8F8F0",
		"editor.foreground": "#F8F8F2",
		"editor.lineHighlightBackground": "#8077A8",
		"editor.selectionBackground": "#8077A8",
		"editorLink.activeForeground": "#db8497",

		"editorLineNumber.foreground": "#F8F8F2",
		"editorWhitespace.foreground": "#A8757B",
		"editorRuler.foreground": "#A8757B",

		"sideBar.background": "#514C66",
		"sideBar.dropBackground": "#464258",
		"sideBar.foreground": "#F8F8F2",
		"activityBar.background": "#464258",
		"activityBarBadge.background": "#c45980",
		"activityBarBadge.foreground": "#F8F8F2",
		"tab.inactiveBackground": "#464258",
		"tab.inactiveForeground": "#F8F8F2",
		"tab.border": "#464258",
		"editorGroupHeader.noTabsBackground": "#464258",
		"editorGroup.border": "#464258",
		"editorGroupHeader.tabsBackground": "#464258",
		"textLink.activeForeground": "#da9da2",
		"textLink.foreground": "#C990A5",
		"titleBar.activeBackground": "#464258",
		"titleBar.activeForeground": "#F8F8F2",
		"titleBar.inactiveBackground": "#3a3553",
		"titleBar.inactiveForeground": "#b9b9b9",
		"terminal.ansiBrightBlack": "#C2FFDF",
		"terminal.ansiRed": "#ffa49f",
		"terminal.ansiGreen": "#9ee79c",

		"statusBar.background": "#d373a3",
		"statusBar.foreground": "#F8F8F2",
		"statusBar.debuggingBackground": "#FFB347",
		"statusBar.debuggingForeground": "#F8F8F2",
		"statusBar.noFolderBackground": "#d373a3",
		"statusBar.noFolderForeground": "#F8F8F2",
		"notifications.background": "#464258",

	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": "comment",
			"settings": {
				"foreground": "#869099"
			}
		},
		{
			"name": "String",
			"scope": "string",
			"settings": {
				"foreground": "#FFF5BA"
			}
		},
		{
			"name": "Number",
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#C5A3FF"
			}
		},
		{
			"name": "Built-in constant",
			"scope": "constant.language",
			"settings": {
				"foreground": "#C5A3FF"
			}
		},
		{
			"name": "User-defined constant",
			"scope": [
				"constant.character",
				"constant.other"
			],
			"settings": {
				"foreground": "#C5A3FF"
			}
		},
		{
			"name": "Variable",
			"scope": "variable",
			"settings": {
				"fontStyle": ""
			}
		},
		{
			"name": "Keyword",
			"scope": "keyword",
			"settings": {
				"foreground": "#FFCBC1"
			}
		},
		{
			"name": "Storage",
			"scope": "storage",
			"settings": {
				"fontStyle": "",
				"foreground": "#FFCBC1"
			}
		},
		{
			"name": "Storage type",
			"scope": "storage.type",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#C2FFDF"
			}
		},
		{
			"name": "Class name",
			"scope": "entity.name.class",
			"settings": {
				"fontStyle": "underline",
				"foreground": "#fabf67"
			}
		},
		{
			"name": "This",
			"scope": [
				"variable.language.this"
			],
			"settings": {
				"foreground": "#debff3"
			}
		},
		{
			"name": "Inherited class",
			"scope": "entity.other.inherited-class",
			"settings": {
				"fontStyle": "italic underline",
				"foreground": "#fabf67"
			}
		},
		{
			"name": "Function name",
			"scope": "entity.name.function",
			"settings": {
				"fontStyle": "",
				"foreground": "#fabf67"
			}
		},
		{
			"name": "Function argument",
			"scope": "variable.parameter",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#7fff94"
			}
		},
		{
			"name": "Tag name",
			"scope": "entity.name.tag",
			"settings": {
				"fontStyle": "",
				"foreground": "#FFCBC1"
			}
		},
		{
			"name": "Tag attribute",
			"scope": "entity.other.attribute-name",
			"settings": {
				"fontStyle": "",
				"foreground": "#fabf67"
			}
		},
		{
			"name": "Library function",
			"scope": "support.function",
			"settings": {
				"fontStyle": "",
				"foreground": "#C2FFDF"
			}
		},
		{
			"name": "Library constant",
			"scope": "support.constant",
			"settings": {
				"fontStyle": "",
				"foreground": "#C2FFDF"
			}
		},
		{
			"name": "Library class/type",
			"scope": [
				"support.type",
				"support.class"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#C2FFDF"
			}
		},
		{
			"name": "Library variable",
			"scope": "support.other.variable",
			"settings": {
				"fontStyle": ""
			}
		},
		{
			"name": "Invalid",
			"scope": "invalid",
			"settings": {
				"fontStyle": "",
				"foreground": "#f79a9a"
			}
		},
		{
			"name": "Invalid deprecated",
			"scope": "invalid.deprecated",
			"settings": {
				"foreground": "#dfd9cb"
			}
		},
		{
			"name": "diff: header",
			"scope": [
				"meta.diff",
				"meta.diff.header"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#fabf67"
			}
		},
		{
			"name": "diff: deleted",
			"scope": "markup.deleted",
			"settings": {
				"foreground": "#fa736e"
			}
		},
		{
			"name": "diff: changed",
			"scope": "markup.changed",
			"settings": {
				"foreground": "#fa9d99"
			}
		},
		{
			"name": "diff: inserted",
			"scope": "markup.inserted",
			"settings": {
				"foreground": "#afecad"
			}
		},
		{
			"name": "Markup Quote",
			"scope": "markup.quote",
			"settings": {
				"foreground": "#86f799"
			}
		},
		{
			"name": "Markup Styling Bold",
			"scope": "markup.bold",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#c2f6ff"
			}
		},
		{
			"name": "Markup Styling Italic",
			"scope": "markup.italic",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#C2FFDF"
			}
		},
		{
			"name": "Markup Inline",
			"scope": "markup.inline.raw",
			"settings": {
				"foreground": "#f5c359"
			}
		},
		{
			"name": "Markup Header",
			"scope": "markup.heading.markdown",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#ddbb88"
			}
		},
		{
			"name": "Source PHP Embedded Begin/End",
			"scope": [
				"text.html.php punctuation.section.embedded.begin.php",
				"text.html.php punctuation.section.embedded.end.php"
			],
			"settings": {
				"foreground": "#c1abe9"
			}
		},
		{
			"name": "Source Java Storage Modifier Import",
			"scope": [
				"source.java storage.modifier.import",
				"source.java storage.modifier.package"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#dda0ad"
			}
		},
		{
			"name": "Javadoc keywords",
			"scope": "source.java keyword.other.documentation.javadoc.java",
			"settings": {
				"foreground": "#ac74cf"
			}
		},
		{
			"name": "Clojure locals",
			"scope": "source.clojure meta.symbol",
			"settings": {
				"foreground": "#b29cc9"
			}
		},
		{
			"name": "Clojure namespace",
			"scope": [
				"source.clojure entity.global",
				"source.clojure meta.definition.global"
			],
			"settings": {
				"foreground": "#cf6e8f"
			}
		},
		{
			"name": "Clojure keyword",
			"scope": "source.clojure constant.keyword",
			"settings": {
				"foreground": "#c19fd8"
			}
		},
		{
			"name": "Clojure namespace",
			"scope": [
				"source.clojure meta.symbol meta.expression",
				"source.clojure meta.vector"
			],
			"settings": {
				"foreground": "#df7e9f"
			}
		},
		{
			"name": "Python cls, self",
			"scope": [
				"source.python variable.language.special.self.python",
				"source.python variable.language.special.cls.python"
			],
			"settings": {
				"foreground": "#b473ff"
			}
		},
		{
			"name": "Python docstring",
			"scope": [
				"source.python string.quoted.docstring.multi.python"
			],
			"settings": {
				"foreground": "#fabf67"
			}
		},
		{
			"name": "Python function name that matches builtins",
			"scope": [
				"source.python meta.function.python support.function.builtin.python"
			],
			"settings": {
				"foreground": "#FFF352"
			}
		},
		{
			"name": "Python logical operator",
			"scope": [
				"source.python keyword.operator.logical.python",
				"source.python keyword.operator.comparison.python"
			],
			"settings": {
				"foreground": "#C5A3FF"
			}
		},
		{
			"name": "Python decorators",
			"scope": [
				"source.python meta.function.decorator.python",
				"source.python entity.name.function.decorator.python"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#C2FFDF"
			}
		},
		{
			"name": "Python function parameter annotation",
			"scope": [
				"source.python meta.function.parameters.python",
				"source.python keyword.operator.unpacking.parameter.python"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#C2FFDF"
			}
		},
		{
			"scope": [
				"string.unquoted.plain.out.yaml"
			],
			"settings": {
				"foreground": "#dec0e2"
			}
		},
		{
			"scope": [
				"string.quoted.double.yaml"
			],
			"settings": {
				"foreground": "#C2FFDF"
			}
		},
		{
			"scope": [
				"comment.line.number-sign.yaml"
			],
			"settings": {
				"foreground": "#a186cf"
			}
		}
	],
	"semanticHighlighting": true
}